<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Desc</th>
      <th>Start date</th>
      <th>Period</th>
      <th>Prize</th>
      <th>Prize num</th>
      <th>Poster</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @competitions.each do |competition| %>
      <tr>
        <td><%= competition.title %></td>
        <td><%= competition.desc %></td>
        <td><%= competition.start_date %></td>
        <td><%= competition.period %></td>
        <td><%= competition.prize %></td>
        <td><%= competition.prize_num %></td>
        <td><%= competition.poster %></td>
        <td><%= link_to 'Show', competition %></td>
        <td><%= link_to 'Edit', edit_competition_path(competition) %></td>
        <td><%= link_to 'Destroy', competition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

